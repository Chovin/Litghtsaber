  end

  sbr.da = lerp(sbr.da, rdir*.05*(ds/10), .5)
  

  sbr.x = lerp(sbr.x, mx, .9)
  sbr.y = lerp(sbr.y, my, .9)

  if (btnp(5)) then
   sbrci = sbrci%#sbrcs + 1
   sbr.c = sbrcs[sbrci]
  end
 else 
  if (btn(0)) sbr.x-=12
  if (btn(1)) sbr.x+=12
  if (btn(2)) sbr.y-=12
  if (btn(3)) sbr.y+=12

